{
  "name": "demo-web-gui",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "cpy&dev": "npm run cpy-unity-build && npm run dev",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "install-unity-webgl-vue" : "npm run pack-unity-webgl-vue && npm install ./externals/unity-webgl-vue/unity-webgl-vue-0.0.0.tgz",
    "update-submodules": "npm run pack-unity-webgl-vue && npm upgrade unity-webgl-vue && npm run cpy-unity-build",
    "pack-unity-webgl-vue": "cd externals/unity-webgl-vue && npm run build && npm pack",
    "cpy-unity-build": "npm run cpy-unity-build:model-diff && npm run cpy-unity-build:simulation",
    "cpy-unity-build:model-diff": "node scripts/copy.js externals/webgl-simulator/Builds/ModelDiff/Build public/UnityBuilds/ModelDiff",
    "cpy-unity-build:simulation": "node scripts/copy.js externals/webgl-simulator/Builds/Simulation/Build public/UnityBuilds/Simulation"
  },
  "dependencies": {
    "@primevue/themes": "^4.1.0",
    "pinia": "^2.1.7",
    "primeicons": "^7.0.0",
    "primevue": "^4.1.0",
    "socket.io-client": "^4.8.1",
    "unity-webgl-vue": "file:externals/unity-webgl-vue/unity-webgl-vue-0.0.0.tgz",
    "vue": "^3.4.29",
    "vue-router": "^4.3.3"
  },
  "devDependencies": {
    "@primevue/auto-import-resolver": "^4.0.1",
    "@rushstack/eslint-patch": "^1.8.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.14.5",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vitejs/plugin-vue-jsx": "^4.0.0",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.23.0",
    "jsdom": "^24.1.0",
    "npm-run-all2": "^6.2.0",
    "postcss": "^8.4.47",
    "prettier": "^3.2.5",
    "sass": "^1.79.5",
    "tailwindcss": "^3.4.13",
    "tailwindcss-primeui": "^0.3.4",
    "typescript": "~5.4.0",
    "unplugin-vue-components": "^0.27.4",
    "vite": "^5.3.1",
    "vitest": "^1.6.0",
    "vue-tsc": "^2.0.21"
  }
}
